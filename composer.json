{
    "name": "anavallasuiza/crudoado",
    "description": "",
    "authors": [
        {
            "name": "Adrian P. Blunier",
            "email": "adrian@blunier.net"
        },
        {
            "name": "Carlos Morales",
            "email": "carlos@anavallasuiza.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/support": "5.1.*",
        "illuminate/translation": "5.1.*",
        "anavallasuiza/adoadomin": "dev-master",
        "ablunier/laravel-database": "dev-master",
        "javiereguiluz/easyslugger": "1.0.*",
        "form-manager/form-manager": "4.7.*"
    },
    "require-dev": {
        "illuminate/database": "^5.1",
        "orchestra/testbench": "~3.1",
        "phpunit/phpunit": "~4.0",
        "mockery/mockery": "0.9.*",
        "whatthejeff/nyancat-phpunit-resultprinter": "~1.2"
    },
    "repositories": [
      {
        "type": "vcs",
        "url" : "https://github.com/anavallasuiza/adoadomin"
      }
    ],
    "autoload": {
        "psr-4": {
            "ANavallaSuiza\\Crudoado\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Crudoado\\Tests\\": "tests"
        }
    },
    "minimum-stability": "stable"
}
