{
    "name": "anavel/crud",
    "description": "The fast and simple CRUD generator for Laravel applications",
    "keywords": [
        "laravel",
        "admin",
        "crud",
        "cms"
    ],
    "authors": [
        {
            "name": "Adrian P. Blunier",
            "email": "adrian@anavallasuiza.com"
        },
        {
            "name": "Carlos Morales",
            "email": "carlos@anavallasuiza.com"
        },
        {
            "name": "Eduardo Salguero",
            "email": "eduardo@anavallasuiza.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/support": "5.1.*",
        "illuminate/translation": "5.1.*",
        "anavel/foundation": "0.1.*",
        "ablunier/laravel-database": "1.2.*",
        "form-manager/form-manager": "4.7.*"
    },
    "require-dev": {
        "illuminate/database": "^5.1",
        "orchestra/testbench": "~3.1",
        "phpunit/phpunit": "~4.0",
        "mockery/mockery": "0.9.*",
        "whatthejeff/nyancat-phpunit-resultprinter": "~1.2",
        "php-mock/php-mock-mockery": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Anavel\\Crud\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Anavel\\Crud\\Tests\\": "tests"
        }
    },
    "minimum-stability": "stable"
}
